{% extends 'base.html.twig' %}

{% block title %}Liste des Articles{% endblock %}

{% block body %}
    <h1>Liste des articles</h1>
    <ul>
        {% for article in articles %}
            <li><a href="{{ path('app_article_id',{id : article.id}) }}">{{ article.titre }} </a></li>
        {% endfor %}

    </ul>
{% endblock %}
